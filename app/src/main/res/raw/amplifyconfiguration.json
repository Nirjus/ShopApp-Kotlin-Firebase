{
    "UserAgent": "aws-amplify-cli/2.0",
    "Version": "1.0",
    "auth": {
        "plugins": {
            "awsCognitoAuthPlugin": {
                "UserAgent": "aws-amplify-cli/2.0",
                "Version": "1.0",
                "IdentityManager": {
                    "Default": {}
                },
                "CredentialsProvider": {
                    "CognitoIdentity": {
                        "Default": {
                            "PoolId": "ap-south-1:9a7c27f2-7147-4de3-9054-a4570b6af3a2",
                            "Region": "ap-south-1",
                            "AllowUnauthenticatedIdentities": true
                        }
                    }
                },
                "Auth": {
                    "Default": {
                        "authenticationFlowType": "USER_SRP_AUTH",
                        "OAuth": {
                            "WebDomain": "your-domain.auth.ap-south-1.amazoncognito.com",
                            "AppClientId": "your-app-client-id",
                            "SignInRedirectURI": "myapp://callback/",
                            "SignOutRedirectURI": "myapp://signout/",
                            "Scopes": [
                                "email",
                                "openid"
                            ]
                        }
                    }
                }
            }
        }
    },
    "storage": {
        "plugins": {
            "awsS3StoragePlugin": {
                "bucket": "my-kotlin-app-bucket",
                "region": "ap-south-1",
                "defaultAccessLevel": "guest"
            }
        }
    }
}
